{
  "name": "xakki/phperrorcatcher",
  "version": "0.5.1",
  "description": "Logger all error in file for PHP",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "xakki",
      "email": "dev@xakki.ru"
    }
  ],
  "support": {
    "issues": "https://github.com/xakki/phperrorcatcher/issues"
  },
  "require": {
    "php": "^5.6",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-pdo": "*",
    "psr/log": "^1.1"
  },
  "suggest": {
    "ext-sockets": "For SyslogStorage support",
    "ext-memcached": "For memcahed cache"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13",
    "vimeo/psalm": "1.1.9"
  },
  "autoload": {
    "psr-4": {
        "Xakki\\PhpErrorCatcher\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "cs-check": "XDEBUG_MODE=off phpcs -ps",
    "cs-fix": "XDEBUG_MODE=off phpcbf -p",
    "psalm": "psalm"
  }
}
